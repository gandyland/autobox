<h2>Edit Service</h2>

<%= form_for @service do |f| %>
<%= f.label :vehicle %>
<%= f.text_field :vehicle %>

<%= f.label :date %>
<%= f.text_field :date %>

<%= f.label :mileage %>
<%= f.text_field :mileage %>

<%= f.label :service %>
<%= f.text_field :service %>

<%= f.label :cost %>
<%= f.text_field :cost %>

<%= f.label :estimate %>
<%= f.text_field :estimate %>

<%= f.label :shop %>
<%= f.text_field :shop %>

<%= f.label :mechanic %>
<%= f.text_field :mechanic %>

<%= f.label :quality %>
<%= f.text_field :quality %>

<%= f.label :overall %>
<%= f.text_field :overall %>

<%= f.label :recommendations %>
<%= f.text_field :recommendations %>

<%= f.label :comments %>
<%= f.text_field :comments %>

<%= f.label :photo_url %>
<%= f.text_field :photo_url %>

<%= f.label :car_id %>
<%= f.text_field :car_id %>

<%= f.submit %>
<% end %>

<%= form_for @service, method: :delete do |f| %>
<%= f.submit "Delete!", class: "delete" %>
<% end %>
